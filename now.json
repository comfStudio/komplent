{
    "version": 2,
    "name": "komplent",
    "routes": [
      { "src": "/static/(.*)", "dest": "/build/$1" },
      { "src": "/(.*)", "dest": "/server/index.js" }
    ],
    "builds": [
      { "src": "dist/*", "use": "@now/static" },
      { "src": "server/index.js", "use": "@now/node" }
    ]
  }